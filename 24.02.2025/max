#include <iostream>
int max_number(int a, int b) {
    if (a > b) {
        return a;
    } else {
        return b;
    }
}
int main() { 
    int num1, num2;
    std::cout << "Enter the numbers: ";
    std::cin >> num1 >> num2;

    int n = max_number(num1, num2); 
    std::cout << "Maximum is: " <<n<< std::endl;

    return 0;
}
