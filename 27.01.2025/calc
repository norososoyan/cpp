#include <iostream>
#include <cmath>

int main() {
    double a, b;
    char c;

    std::cin >> a >> c >> b;

    switch (c) {
        case '+':
            std::cout << a + b << '\n';
            break;
        case '-':
            std::cout << a - b << '\n';
            break;
        case '/':
            if (b != 0)
                std::cout << a / b << '\n';
            else
                std::cout << "Թիվը չի բաժանվում 0-ի\n";
            break;
        case '*':
            std::cout << a * b << '\n';
            break;
        default:
            std::cout << "Սխալ\n";
    }

    return 0;
}
